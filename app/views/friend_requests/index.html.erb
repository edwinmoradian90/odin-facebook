<% if @incoming.any? %>
  <h1>All friend requests</h1>
  <% @incoming.each do |request| %>
    <ul>
      <li>
        <%= request.user.first_name %>
        <%= request.user.last_name %>
        <%= link_to 'Accept', friend_request_path(request.friend_id), method: 'put', class: 'btn btn-success' %>
      </li>
    </ul>
  <% end %>
<% else %>
  <h1>No new friend requests... :(</h1>
<% end %>